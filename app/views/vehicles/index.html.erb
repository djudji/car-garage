<p id="notice"><%= notice %></p>

<h1>Listing Vehicles</h1>

<table class="table">
  <thead>
    <tr>
      <th>Parked</th>
      <th>Vehicle Type</th>
      <th>Licence Plate</th>
      <th>Slot</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.parked %></td>
        <td><%= vehicle.veh_type %></td>
        <td><%= vehicle.lic_plate %></td>
        <td><%= vehicle.slot.try(:name) %></td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
